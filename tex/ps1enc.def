\ProvidesFile{PS1enc.def}[1994/09/01]
\DeclareFontEncoding{PS1}{}{}
\DeclareTextAccent{\`}{PS1}{193}
\DeclareTextAccent{\'}{PS1}{194}
\DeclareTextAccent{\^}{PS1}{195}
\DeclareTextAccent{\~}{PS1}{196}
