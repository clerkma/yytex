\DeclareTextAccent{\=}{PS1}{197}
\DeclareTextAccent{\.}{PS1}{199}
\DeclareTextCommand{\b}{PS1}[1]
   {\oalign{#1\crcr\hidewidth
    \vbox to.2ex{\hbox{\char197}\vss}\hidewidth}}
\DeclareTextCommand{\c}{PS1}[1]
   {\setbox\z@\hbox{#1}\ifdim\ht\z@=1ex\accent203 #1%
     \else{\ooalign{\hidewidth\char203\hidewidth
        \crcr\unhbox\z@}}\fi}
\DeclareTextCommand{\d}{PS1}[1]
   {\oalign{#1\crcr\hidewidth.\hidewidth}}
\DeclareTextCommand{\k}{PS1}[1]
   {\oalign{#1\crcr\char206\hidewidth}}
