\DeclareTextCommand{\b}{PA1}[1]
   {\oalign{#1\crcr\hidewidth
    \vbox to.2ex{\hbox{\char175}\vss}\hidewidth}}
\DeclareTextCommand{\c}{PA1}[1]
   {\setbox\z@\hbox{#1}\ifdim\ht\z@=1ex\accent184 #1%
     \else{\ooalign{\hidewidth\char184\hidewidth
        \crcr\unhbox\z@}}\fi}
\DeclareTextCommand{\d}{PA1}[1]
   {\oalign{#1\crcr\hidewidth.\hidewidth}}
